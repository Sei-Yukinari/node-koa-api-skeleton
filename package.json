{
  "name": "node-koa-api-skeleton",
  "version": "0.0.1",
  "private": true,
  "description": "A node-koa-api-skeleton for Web APIs using Node And Koa ",
  "main": "index.js",
  "scripts": {
    "start": "node -r babel-register ./index.js",
    "dev": "NODE_ENV=test `npm bin`/babel-watch ./index.js",
    "lint": "`npm bin`/eslint ./;exit 0;",
    "mocha": "`npm bin`/mocha --compilers js:babel-core/register --reporter nyan --recursive;exit 0;",
    "test": "npm run flow && npm run lint && npm run mocha",
    "flow": "flow;exit 0;",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "db:migrate": "cd ./src/infrastructure/database && `npm bin`/sequelize db:migrate;",
    "db:migrate:undo": "cd ./src/infrastructure/database && `npm bin`/sequelize db:migrate:undo;"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^3.0.4",
    "awilix-koa": "^2.1.0",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.23.0",
    "babel-watch": "^2.0.6",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "flow-bin": "^0.56.0",
    "http-status": "^1.0.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-methodoverride": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "linq": "^3.0.7",
    "log4js": "^2.5.2",
    "moment": "^2.15.2",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.32.2",
    "sequelize-cli": "^3.0.0",
    "structure": "^1.2.1",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-typed": "^2.3.0",
    "mocha": "^3.1.1",
    "nock": "^9.0.13",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  }
}
